   nat = 2
/

&electrons
   mixing_beta = 0.3
   conv_thr = 1.0e-8
   mixing_mode = 'plain'
   diagonalization = 'david'
/

&ions
   ion_dynamics = 'bfgs'
/

&cell
   cell_dynamics = 'bfgs'
   press = 0
/

