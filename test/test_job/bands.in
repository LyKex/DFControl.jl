&control
   prefix = 'pwo'
   verbosity = 'high'
   calculation = 'bands'
   outdir = './'
   pseudo_dir = './'
   disk_io = 'low'
   restart_mode = 'from_scratch'
/

&system
   ecutwfc = 28
   starting_magnetization(1) = -1.0e-5
   occupations = 'smearing'
   nbnd = 48
   nosym = true
   nspin = 2
   ntyp = 2
   smearing = 'cold'
   degauss = 0.01
   ibrav = 0
